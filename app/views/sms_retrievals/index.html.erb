<p id="notice"><%= notice %></p>

<h1>Listing Sms Retrievals</h1>

<table>
  <thead>
    <tr>
      <th>Phone number</th>
      <th>Room</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sms_retrievals.each do |sms_retrieval| %>
      <tr>
        <td><%= sms_retrieval.phone_number %></td>
        <td><%= sms_retrieval.room_id %></td>
        <td><%= link_to 'Show', sms_retrieval %></td>
        <td><%= link_to 'Edit', edit_sms_retrieval_path(sms_retrieval) %></td>
        <td><%= link_to 'Destroy', sms_retrieval, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sms retrieval', new_sms_retrieval_path %>
